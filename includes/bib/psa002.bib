  pmid = {27784749},
  keywords = {facial feedback hypothesis,many-labs,preregistration,replication},
  file = {D\:\\core\\reading\\psychological science\\Wagenmakers 等。 - 2016 - Registered Replication Report Strack, Martin, & St.pdf;D\:\\core\\Version_control\\zotero\\storage\\QHJQTV6W\\1745691616674458.html}

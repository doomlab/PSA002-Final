  shorttitle = {Cross-{{Linguistic Differences}} in {{Side Assignment}} to {{Objects}} and {{Interpretation}} of {{Spatial Relations}}},
  booktitle = {Spatial {{Cognition XII}}},
  author = {Stoltmann, Katarzyna and Fuchs, Susanne and Krifka, Manfred},
